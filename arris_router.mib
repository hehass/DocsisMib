arrisRouterAdvancedRadiusCfgSettingsTable OBJECT-TYPE
	SYNTAX		SEQUENCE OF ArrisRouterAdvancedRadiusCfgSettingsEntry
	MAX-ACCESS 	not-accessible
	STATUS		current
	DESCRIPTION

	"This table assigns Advanced Radius Settings to be applied to Radius Servers per BSS.
	An entry exists in this table for each row in the arrisRouterBSSTable
	for which arrisRouterWiFiSecurityMode is set to wpaEnterprise(4),
	wpa2Enterprise(5), or radiusWep(6) when arrisRouterAdvancedRadiusCfg is enabled AND
	arrisRouterAdvancedRadiusCfg_SettingsEnable is true."

	::= { arrisRouterAdvancedRadiusCfg 11 }

arrisRouterAdvancedRadiusCfgSettingsEntry OBJECT-TYPE
	SYNTAX		ArrisRouterAdvancedRadiusCfgSettingsEntry
	MAX-ACCESS      not-accessible
	STATUS		current
	DESCRIPTION

	"This table assigns Advanced Radius Settings to be applied to Radius Servers per BSS.
	An entry exists in this table for each row in the arrisRouterBSSTable
	for which arrisRouterWiFiSecurityMode is set to wpaEnterprise(4),
	wpa2Enterprise(5), or radiusWep(6) when arrisRouterAdvancedRadiusCfg is enabled AND
	arrisRouterAdvancedRadiusCfg_SettingsEnable is true."

	INDEX { ifIndex }
	::= { arrisRouterAdvancedRadiusCfgSettingsTable 1 }

ArrisRouterAdvancedRadiusCfgSettingsEntry ::=
	SEQUENCE   {
		arrisRouterAdvancedRadiusCfgSettingsServerRetries               Unsigned32,
		arrisRouterAdvancedRadiusCfgSettingsServerRequestTimeout        Unsigned32,
		arrisRouterAdvancedRadiusCfgSettingsPMKLifetime                 Unsigned32,
		arrisRouterAdvancedRadiusCfgSettingsPMKCaching                  Unsigned32,
		arrisRouterAdvancedRadiusCfgSettingsPMKCacheInterval            Unsigned32,
		arrisRouterAdvancedRadiusCfgSettingsMaxAuthAttempts             Unsigned32,
		arrisRouterAdvancedRadiusCfgSettingsBlacklistTimeout            Unsigned32,
		arrisRouterAdvancedRadiusCfgSettingsIDRequestRetryInterval      Unsigned32,
		arrisRouterAdvancedRadiusCfgSettingsQuietPeriodAfterFailedAuth  Unsigned32
		}

arrisRouterAdvancedRadiusCfgSettingsServerRetries OBJECT-TYPE
