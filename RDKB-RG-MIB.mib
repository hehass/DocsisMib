    ::= { rdkbRgDeviceBase 1005 }


rdkbRgPlatformCapable OBJECT-TYPE
    SYNTAX      TruthValue
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
                " indicates RDKB capability."
        DEFVAL { true }
    ::= { rdkbRgDeviceBase 1006 }

rdkbRgCaptivePortalEnable OBJECT-TYPE
    SYNTAX      TruthValue
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
                "Indicates whether RDKB device is in captive portal mode.
                 true: indicates device is in captive portal
