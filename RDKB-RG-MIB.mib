RdkbRgIpMgmtLanEntry ::= SEQUENCE {
    rdkbRgIpMgmtLanMode              INTEGER,
    rdkbRgIpMgmtLanNetwork           IpAddress,
    rdkbRgIpMgmtLanNetworksAllow     INTEGER,
    rdkbRgIpMgmtLanSubnetMask        IpAddress,
