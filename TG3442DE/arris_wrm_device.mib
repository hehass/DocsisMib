--
-- The following group describes the MIB objects which control RIPv2 parameters
-- in the WRM device, that are not covered by the RFC rip2 MIBs
--

arrisWrmDevRip2Entry OBJECT-TYPE
     SYNTAX      ArrisWrmDevRip2Entry
     MAX-ACCESS  not-accessible
     STATUS      current
     DESCRIPTION
         "This table contains MIB objects which control RIPv2 parameters
          in the WRM device that are not covered by 'rip2IfConfTable'."
     INDEX { arrisWrmDevRip2KeyId  }
     ::= {  arrisWrmDevRip2Table 1 }

ArrisWrmDevRip2Entry ::= SEQUENCE {
         arrisWrmDevRip2KeyId             Integer32,
         arrisWrmDevRip2AdvertisementInt  Integer32,
         arrisWrmDevRipTransmitStatus     INTEGER,
         arrisWrmDevRipReceiveStatus      INTEGER
     }

arrisWrmDevRip2KeyId OBJECT-TYPE
     SYNTAX  Integer32
     MAX-ACCESS read-create
     STATUS current
     DESCRIPTION
         "An unsigned 8-bit field that contains the Key Identifier
          or Key-ID. This identifies the key used to create the
          Authentication Data for this RIP-2 message whenever
          rip2IfConfAuthType has a value other than noAuthentication."
     ::= { arrisWrmDevRip2Entry 1 }

arrisWrmDevRip2AdvertisementInt OBJECT-TYPE
     SYNTAX  Integer32
     MAX-ACCESS read-create
     STATUS current
     DESCRIPTION
         "An unsigned 16-bit field that contains the interval in seconds
          between transmits of routing table updates."
     ::= { arrisWrmDevRip2Entry 2 }

arrisWrmDevRipTransmitStatus OBJECT-TYPE
     SYNTAX INTEGER {
         active(1),
         inactive(2)
     }
     MAX-ACCESS read-only
     STATUS current
     DESCRIPTION
         "Active when the WRM is actively transmitting RIP updates.  In order for
          arrisWrmDevRipTransmitStatus to be active, rip2IfConfSend must be active
          and arrisWrmDevNATEnable must be disabled."
     ::= { arrisWrmDevRip2Entry 3 }


arrisWrmDevRipReceiveStatus OBJECT-TYPE
     SYNTAX INTEGER {
         active(1),
         inactive(2)
     }
     MAX-ACCESS read-only
     STATUS current
     DESCRIPTION
         "Active when the WRM is actively transmitting RIP updates.  In order for
          arrisWrmDevRipReceiveStatus to be active, rip2IfConfReceive must be active."
     ::= { arrisWrmDevRip2Entry 4 }

