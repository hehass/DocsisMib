--
-- The following MIB does the equivalent of the CLI command "sdc save"
--

arrisWrmDevSaveSettings OBJECT-TYPE
     SYNTAX INTEGER {
         true(1),
         false(2)
     }
     MAX-ACCESS read-write
     STATUS current
     DESCRIPTION
         "If set to True(1), the WRM will save settings and reboot.
          When read, False(2) will always be returned"
     ::= { arrisWrmDevOperationalSetup 1 }

--
-- The following group describes the MIB objects which control LAN IP parameters
-- in the Wrm device.
--

arrisWrmDevNATEnable OBJECT-TYPE
     SYNTAX INTEGER {
         enable(1),
         disable(2)
     }
     MAX-ACCESS read-write
     STATUS current
     DESCRIPTION
         "This MIB object is used to enable or disable NAT (Network
          Address Translation) on the WRM.

          Note: NAT must be disabled in order to transmit RIP
          information."
     ::= { arrisWrmDevLan 1 }

arrisWrmDevLanIpAddr OBJECT-TYPE
     SYNTAX  IpAddress
     MAX-ACCESS read-write
     STATUS current
     DESCRIPTION
         "This is the IP address for the LAN interface of the WRM.
          All IP addresses assigned by the WRM's dhcp server are based
          from this IP address."
     ::= { arrisWrmDevLan 2 }

arrisWrmDevLanIpMask OBJECT-TYPE
     SYNTAX  IpAddress
     MAX-ACCESS read-write
     STATUS current
     DESCRIPTION
         "This is the subnet Mask for the LAN interface of the WRM.
          Together with arrisWrmDevLanIpAddr, this MIB defines
          the subnet on the LAN interface of the WRM."
     ::= { arrisWrmDevLan 3 }

arrisWrmDevLanIpPoolStart OBJECT-TYPE
     SYNTAX  Integer32
     MAX-ACCESS read-write
     STATUS current
     DESCRIPTION
         "An  unsigned 8-bit field that determines the first IP address
          in the WRM's DHCP IP Address pool.

          The value contained in this MIB only determines the last
          8 bits of the IP address.  The first 3 bytes (24 bits) of
          the IP address come from the first 3 bytes of
          arrisWrmDevLanIpAddr."
     ::= { arrisWrmDevLan 4 }

arrisWrmDevLanIpPoolEnd OBJECT-TYPE
     SYNTAX  Integer32
     MAX-ACCESS read-write
     STATUS current
     DESCRIPTION
         "An  unsigned 8-bit field that determines the last IP address
          in the WRM's DHCP IP Address pool.

          The value contained in this MIB only determines the last
          8 bits of the IP address.  The first 3 bytes (24 bits) of
          the IP address come from the first 3 bytes of
          arrisWrmDevLanIpAddr."
     ::= { arrisWrmDevLan 5 }

arrisWrmDevLanDhcp OBJECT-TYPE
     SYNTAX INTEGER {
          enable(1),
          disable(2)
     }
     MAX-ACCESS read-write
     STATUS current
     DESCRIPTION
         "LAN DHCP server global enable."
     ::= { arrisWrmDevLan 6 }

arrisWrmDevLanLease OBJECT-TYPE
     SYNTAX INTEGER {
          forever(0),
          halfHour(1),
          oneHour(2),
          twoHours(3),
          halfDay(4),
          oneDay(5),
          twoDays(6),
          oneWeek(7),
          twoWeeks(8)
     }
     MAX-ACCESS read-write
     STATUS current
     DESCRIPTION
         "Ammount of time in the LAN DHCP server lease time field"
     ::= { arrisWrmDevLan 7 }

arrisWrmDevLanDomain OBJECT-TYPE
     SYNTAX OCTET STRING
     MAX-ACCESS read-write
     STATUS current
     DESCRIPTION
         "Domain name used by the LAN DHCP server"
     ::= { arrisWrmDevLan 8 }


    arrisWrmDevLanIpReserve OBJECT IDENTIFIER
     ::= { arrisWrmDevLan 9 }


        arrisWrmDevLanIpReserveTable OBJECT-TYPE
            SYNTAX SEQUENCE OF ArrisWrmDevLanIpReserveEntry
            MAX-ACCESS not-accessible
            STATUS current
            DESCRIPTION
                "Table of reserve ip."
            ::= { arrisWrmDevLanIpReserve 1 }


        arrisWrmDevLanIpReserveEntry OBJECT-TYPE
            SYNTAX ArrisWrmDevLanIpReserveEntry
            MAX-ACCESS not-accessible
            STATUS current
            DESCRIPTION
                "Table of reserve ip."
            INDEX { arrisWrmDevLanIpReserveIndex }
            ::= { arrisWrmDevLanIpReserveTable 1 }


        ArrisWrmDevLanIpReserveEntry ::=
            SEQUENCE {
                arrisWrmDevLanIpReserveIndex
                    Integer32,
                arrisWrmDevLanIpReserveHostname
                    OCTET STRING,
                arrisWrmDevLanIpReserveIp
                    IpAddress,
                arrisWrmDevLanIpReserveMac
                    OCTET STRING
             }

        arrisWrmDevLanIpReserveIndex OBJECT-TYPE
            SYNTAX Integer32
            MAX-ACCESS read-only
            STATUS current
            DESCRIPTION
                "Index of table entry."
            ::= { arrisWrmDevLanIpReserveEntry 1 }


        arrisWrmDevLanIpReserveHostname OBJECT-TYPE
            SYNTAX OCTET STRING (SIZE (0..15))
            MAX-ACCESS read-write
            STATUS current
            DESCRIPTION
                "Hostname can't contain `~!@#$%^&*()=+[]{}\|;:<>?/"
            ::= { arrisWrmDevLanIpReserveEntry 2 }


        arrisWrmDevLanIpReserveIp OBJECT-TYPE
            SYNTAX IpAddress
            MAX-ACCESS read-write
            STATUS current
            DESCRIPTION
                "Client will always receive the this IP address each time it accesses the DHCP server."
            ::= { arrisWrmDevLanIpReserveEntry 3 }


        arrisWrmDevLanIpReserveMac OBJECT-TYPE
            SYNTAX OCTET STRING
            MAX-ACCESS read-write
            STATUS current
            DESCRIPTION
                "Client MAC address.
                 example 00aabbccddee"
            ::= { arrisWrmDevLanIpReserveEntry 4 }


        arrisWrmDevLanIpReserveDelByIndex OBJECT-TYPE
            SYNTAX INTEGER (0..255)
            MAX-ACCESS read-write
            STATUS current
            DESCRIPTION
                "Remove reserve ip from table.
                index value please see arrisWrmDevLanIpReserveIndex"
            ::= { arrisWrmDevLanIpReserve 2 }


--
-- The following group describes the MIB objects which control WLAN IP parameters
-- in the Wrm device.
--
arrisWrmDevWlanAp OBJECT-TYPE
     SYNTAX INTEGER {
         enable(1),
         disable(2)
     }
     MAX-ACCESS read-write
     STATUS current
     DESCRIPTION
         "Enable/Disable"
     ::= { arrisWrmDevWlan 1 }

arrisWrmDevWlanApDhcpc OBJECT-TYPE
     SYNTAX INTEGER {
         enable(1),
         disable(2)
     }
     MAX-ACCESS read-write
     STATUS current
     DESCRIPTION
         "Enable the DHCP client in the WRM.  This client typically used in AP mode."
     ::= { arrisWrmDevWlan 2 }

arrisWrmDevWlanApConfigTable OBJECT-TYPE
    SYNTAX SEQUENCE OF ArrisWrmDevWlanApConfigEntry
    MAX-ACCESS not-accessible
    STATUS current
    DESCRIPTION
        "This table contains MIB objects which control WLAN basic
         parameters not covered in the WLAN security mode parameters."
    ::= { arrisWrmDevWlan 3 }

arrisWrmDevWlanApConfigEntry OBJECT-TYPE
     SYNTAX      ArrisWrmDevWlanApConfigEntry
     MAX-ACCESS  not-accessible
     STATUS      current
     DESCRIPTION
         "This table contains MIB objects which control WLAN basic
          parameters,which does not cover WLAN security mode parameters."
     INDEX { arrisWrmDevWlanApConfigIndex  }
     ::= {  arrisWrmDevWlanApConfigTable 1 }

ArrisWrmDevWlanApConfigEntry ::= SEQUENCE {
         arrisWrmDevWlanApConfigIndex     INTEGER,
         arrisWrmDevWlanApConfigChannel   INTEGER,
         arrisWrmDevWlanApConfigMode      Integer32,
         arrisWrmDevWlanApConfigSsid      OCTET STRING,
         arrisWrmDevWlanApConfigSSidBroad INTEGER,
         arrisWrmDevWlanApConfigProtec    INTEGER,
         arrisWrmDevWlanApConfigQos       INTEGER,
         arrisWrmDevWlanApConfigLanAccess INTEGER
     }

arrisWrmDevWlanApConfigIndex OBJECT-TYPE
     SYNTAX INTEGER
     MAX-ACCESS read-only
     STATUS current
     DESCRIPTION
         "The index value which uniquely identifies the
     SSID NO. to which this entry is applicable."
     ::= { arrisWrmDevWlanApConfigEntry 1 }

arrisWrmDevWlanApConfigChannel OBJECT-TYPE
     SYNTAX INTEGER {
         auto(0),
         channel-1(1),
         channel-2(2),
         channel-3(3),
         channel-4(4),
         channel-5(5),
         channel-6(6),
         channel-7(7),
         channel-8(8),
         channel-9(9),
         channel-10(10),
         channel-11(11)
     }
     MAX-ACCESS read-write
     STATUS current
     DESCRIPTION "auto | 1-11"
     ::= { arrisWrmDevWlanApConfigEntry 2 }

arrisWrmDevWlanApConfigMode OBJECT-TYPE
     SYNTAX Integer32
     MAX-ACCESS read-write
     STATUS current
     DESCRIPTION
     "802.11 mode:
      0 - OFF
      1 - 802.11g and 802.11b
      2 - 802.11g only
      3 - 802.11b only"
     ::= { arrisWrmDevWlanApConfigEntry 3 }

arrisWrmDevWlanApConfigSsid OBJECT-TYPE
     SYNTAX OCTET STRING
     MAX-ACCESS read-write
     STATUS current
     DESCRIPTION  "SSID of this wireless channel"
     ::= { arrisWrmDevWlanApConfigEntry 4 }

arrisWrmDevWlanApConfigSSidBroad OBJECT-TYPE
     SYNTAX INTEGER {
         enable(1),
         disable(2)
     }
     MAX-ACCESS read-write
     STATUS current
     DESCRIPTION "Broadcast SSID enable/disable."
     ::= { arrisWrmDevWlanApConfigEntry 5 }

arrisWrmDevWlanApConfigProtec OBJECT-TYPE
     SYNTAX INTEGER {
         on(1),
         off(2)
     }
     MAX-ACCESS read-write
     STATUS current
     DESCRIPTION
         "Protected Mode
          If your wireless network has interference from other wireless networks
          or devices such as wireless telephones, turning on Protected Mode may improve
          performance. Otherwise, leave this option off for best performance."
     ::= { arrisWrmDevWlanApConfigEntry 6 }

arrisWrmDevWlanApConfigQos OBJECT-TYPE
     SYNTAX INTEGER {
         on(1),
         off(2)
     }
     MAX-ACCESS read-write
     STATUS current
     DESCRIPTION
         "The Quality of Service (QoS) feature provides enhanced performance over the wireless
          network for streaming multimedia and similar services. Its primary goal is to set priority
          on dedicated bandwidth, control jitter and latency (required by some real-time and interactive
          traffic) and improve loss characteristics. Equally important is to make sure that setting
          priority for one or more flows does not completely block other flows. "
     ::= { arrisWrmDevWlanApConfigEntry 7 }

arrisWrmDevWlanApConfigLanAccess OBJECT-TYPE
     SYNTAX INTEGER {
         enable(1),
         disable(2)
     }
     MAX-ACCESS read-write
     STATUS current
     DESCRIPTION
         " Enable/Disable WLAN access to the WRM and devices connected
           to the WRM LAN ethernet ports.
           Default value is enabled. "
     ::= { arrisWrmDevWlanApConfigEntry 8 }

arrisWrmDevWlanApSecuConfigTable OBJECT-TYPE
    SYNTAX SEQUENCE OF ArrisWrmDevWlanApSecuConfigEntry
    MAX-ACCESS not-accessible
    STATUS current
    DESCRIPTION
        "This table contains MIB objects which control WLAN
         security parameters,which does not cover them in
         'arrisWrmDevWlanApSecuConfigTable'."
    ::= { arrisWrmDevWlan 4 }

arrisWrmDevWlanApSecuConfigEntry OBJECT-TYPE
     SYNTAX      ArrisWrmDevWlanApSecuConfigEntry
     MAX-ACCESS  not-accessible
     STATUS      current
     DESCRIPTION
         "This table contains MIB objects which control WLAN
          security parameters,which does not cover them in
          'arrisWrmDevWlanApSecuConfigTable'."
     INDEX { arrisWrmDevWlanApSecuConfigIndex  }
     ::= {  arrisWrmDevWlanApSecuConfigTable 1 }

ArrisWrmDevWlanApSecuConfigEntry ::= SEQUENCE {
         arrisWrmDevWlanApSecuConfigIndex        INTEGER,
         arrisWrmDevWlanApSecuConfigEnable       INTEGER,
         arrisWrmDevWlanApSecuConfigWep          INTEGER,
         arrisWrmDevWlanApSecuConfigWepK-type    INTEGER,
         arrisWrmDevWlanApSecuConfigWepK-index   Integer32,
         arrisWrmDevWlanApSecuConfigWepK-gen     OCTET STRING,
         arrisWrmDevWlanApSecuConfigWepK-man     OCTET STRING,
         arrisWrmDevWlanApSecuConfigWpaEncry     INTEGER,
         arrisWrmDevWlanApSecuConfigWpaphrase    OCTET STRING,
         arrisWrmDevWlanApSecuConfigWpaObsecu    INTEGER,
         arrisWrmDevWlanApSecuConfigWpa2phrase   OCTET STRING,
         arrisWrmDevWlanApSecuConfigWpa2Obsecu   INTEGER
     }

arrisWrmDevWlanApSecuConfigIndex OBJECT-TYPE
     SYNTAX INTEGER
     MAX-ACCESS read-only
     STATUS current
     DESCRIPTION
         "The index value which uniquely identifies the
     SSID NO. to which this entry is applicable."
     ::= { arrisWrmDevWlanApSecuConfigEntry 1 }

arrisWrmDevWlanApSecuConfigEnable OBJECT-TYPE
     SYNTAX INTEGER {
         disable(0),
         wep(1),
         wpa-psk(2),
         wpa2-psk(3),
         wpa-wpa2-psk(4),
         wpa-enterprise(5),
         wpa2-enterprise(6),
         wpa-wpa2-enterprise(7)
     }
     MAX-ACCESS read-write
     STATUS current
     DESCRIPTION
         "Using encryption can help secure your wireless network.
          Select an encrytpion mode that is supported by all network
          devices on your wireless network."
     ::= { arrisWrmDevWlanApSecuConfigEntry 2 }

arrisWrmDevWlanApSecuConfigWep OBJECT-TYPE
     SYNTAX INTEGER {
     	 open(1),
     	 share(2),
         auto(3)
     }
     MAX-ACCESS read-write
     STATUS current
     DESCRIPTION
         "WEP (Wired Equivalent Privacy) was the standard for wireless encryption.
          WEP requires you to enter hex digits (0~9, A~F), or generate the key by
          using the PassPhrase/Generate method."
     ::= { arrisWrmDevWlanApSecuConfigEntry 3 }

arrisWrmDevWlanApSecuConfigWepK-type OBJECT-TYPE
     SYNTAX INTEGER {
         bit-64(1),
         bit-128(2)
     }
     MAX-ACCESS read-write
     STATUS current
     DESCRIPTION
         "Configure WEP encryption to be either 128 or 64 bit encryption."
     ::= { arrisWrmDevWlanApSecuConfigEntry 4 }

arrisWrmDevWlanApSecuConfigWepK-index OBJECT-TYPE
     SYNTAX Integer32
     MAX-ACCESS read-write
     STATUS current
     DESCRIPTION "Select between WEP keys 1-4"
     ::= { arrisWrmDevWlanApSecuConfigEntry 5 }

arrisWrmDevWlanApSecuConfigWepK-gen OBJECT-TYPE
     SYNTAX OCTET STRING
     MAX-ACCESS read-write
     STATUS current
     DESCRIPTION
        "Passphrase used to generate WEP key.  Resulting WEP key will
         be shown in arrisWrmDevWlanApSecuConfigWepK-man"
     ::= { arrisWrmDevWlanApSecuConfigEntry 6 }

arrisWrmDevWlanApSecuConfigWepK-man OBJECT-TYPE
     SYNTAX OCTET STRING
     MAX-ACCESS read-write
     STATUS current
     DESCRIPTION "Manually enter WEP key"
     ::= { arrisWrmDevWlanApSecuConfigEntry 7 }

arrisWrmDevWlanApSecuConfigWpaEncry OBJECT-TYPE
     SYNTAX INTEGER {
        tkip(0),
        aes(1),
        tkip-aes(2)
     }
     MAX-ACCESS read-write
     STATUS current
     DESCRIPTION
         "Encryption Technique: If using WPA-PSK, select the desired encryption
          method - TKIP or AES. WPA2-PSK always uses AES encryption.
          WPA-PSK + WPA2-PSK uses TKIP + AES."
     ::= { arrisWrmDevWlanApSecuConfigEntry 8 }

arrisWrmDevWlanApSecuConfigWpaphrase OBJECT-TYPE
     SYNTAX OCTET STRING
     MAX-ACCESS read-write
     STATUS current
     DESCRIPTION
         "Pre-shared Key (PSK): All clients must use the same PSK. The PSK can be any
          word or phrase from 8 up to 63 characters. The passphrase is case-sensitive
          (.n. is different than .N..) Use a non-obvious word or phrase; the easiest
          way to break your security is for someone to guess your PSK."
     ::= { arrisWrmDevWlanApSecuConfigEntry 9 }

arrisWrmDevWlanApSecuConfigWpaObsecu OBJECT-TYPE
     SYNTAX INTEGER {
          enable(1),
          disable(2)
     }
     MAX-ACCESS read-write
     STATUS current
     DESCRIPTION
         "Used for the WebGUI: When enabled, the Password (PSK) displays '*' for all characters."
     ::= { arrisWrmDevWlanApSecuConfigEntry 10 }

arrisWrmDevWlanApSecuConfigWpa2phrase OBJECT-TYPE
     SYNTAX OCTET STRING
     MAX-ACCESS read-write
     STATUS current
     DESCRIPTION
         "Pre-shared Key (PSK): All clients must use the same PSK. The PSK can be any
          word or phrase from 8 up to 63 characters. The passphrase is case-sensitive
          (.n. is different than .N..) Use a non-obvious word or phrase; the easiest
          way to break your security is for someone to guess your PSK."
     ::= { arrisWrmDevWlanApSecuConfigEntry 11 }

arrisWrmDevWlanApSecuConfigWpa2Obsecu OBJECT-TYPE
     SYNTAX INTEGER {
          enable(1),
          disable(2)
     }
     MAX-ACCESS read-write
     STATUS current
     DESCRIPTION
         "Used for the WebGUI: When enabled, the Password (PSK) displays '*' for all characters."
     ::= { arrisWrmDevWlanApSecuConfigEntry 12 }

arrisWrmDevWlanApSecu8021xConfigTable OBJECT-TYPE
    SYNTAX SEQUENCE OF ArrisWrmDevWlanApSecu8021xConfigEntry
    MAX-ACCESS not-accessible
    STATUS current
    DESCRIPTION
        "This table contains MIB objects which control WLAN
         security 8021x parameters,which covers WPA Enterprise."
    ::= { arrisWrmDevWlan 5 }

arrisWrmDevWlanApSecu8021xConfigEntry OBJECT-TYPE
     SYNTAX      ArrisWrmDevWlanApSecu8021xConfigEntry
     MAX-ACCESS  not-accessible
     STATUS      current
     DESCRIPTION
         "This table contains MIB objects which control WLAN
          security 8021x parameters,which covers WPA Enterprise."
     INDEX { arrisWrmDevWlanApSecu8021xConfigIndex  }
     ::= {  arrisWrmDevWlanApSecu8021xConfigTable 1 }

ArrisWrmDevWlanApSecu8021xConfigEntry ::= SEQUENCE {
         arrisWrmDevWlanApSecu8021xConfigIndex        INTEGER,
         arrisWrmDevWlanApSecu8021xConfigServer       IpAddress,
         arrisWrmDevWlanApSecu8021xConfigShareKey     OCTET STRING,
         arrisWrmDevWlanApSecu8021xConfigRadiusPort   INTEGER
     }

arrisWrmDevWlanApSecu8021xConfigIndex OBJECT-TYPE
     SYNTAX INTEGER
     MAX-ACCESS read-only
     STATUS current
     DESCRIPTION
         "The index value which uniquely identifies the
     Radius Server selection to which this entry is applicable."
     ::= { arrisWrmDevWlanApSecu8021xConfigEntry 1 }

arrisWrmDevWlanApSecu8021xConfigServer OBJECT-TYPE
     SYNTAX IpAddress
     MAX-ACCESS read-write
     STATUS current
     DESCRIPTION
         "Primary Radius Server: This field is required. Enter the IP
          address of the primaray Radius Server on your LAN."
     ::= { arrisWrmDevWlanApSecu8021xConfigEntry 2 }

arrisWrmDevWlanApSecu8021xConfigShareKey OBJECT-TYPE
     SYNTAX OCTET STRING
     MAX-ACCESS read-write
     STATUS current
     DESCRIPTION
         "Shared Key: Enter the desired value for the Shared Key.
          This must match the value used on the Radius server. "
     ::= { arrisWrmDevWlanApSecu8021xConfigEntry 3 }

arrisWrmDevWlanApSecu8021xConfigRadiusPort OBJECT-TYPE
     SYNTAX INTEGER
     MAX-ACCESS read-write
     STATUS current
     DESCRIPTION
         "Radius Port: Enter the port number used for connections
          to the Radius Server.If unsure what the Radius server port
          number is, the default Radius port number is 1812 (as
          specified in RFC 2138). "
     ::= { arrisWrmDevWlanApSecu8021xConfigEntry 4 }

arrisWrmDevWlanApPowerLevel OBJECT-TYPE
     SYNTAX INTEGER {
         high(0),
         middle(1),
         low(2)
     }
     MAX-ACCESS read-write
     STATUS current
     DESCRIPTION
         "adjust wireless power level."
     ::= { arrisWrmDevWlan 6 }

arrisWrmDevWlanApIsolationInter OBJECT-TYPE
     SYNTAX INTEGER {
         enable(1),
         disable(2)
     }
     MAX-ACCESS read-write
     STATUS current
     DESCRIPTION
         "Disable/Enable access to allow that the WLAN PCs in the same SSID can
          communicate with each other."
     ::= { arrisWrmDevWlan 7 }

arrisWrmDevWlanApIsolationExtra OBJECT-TYPE
     SYNTAX INTEGER {
         enable(1),
         disable(2)
     }
     MAX-ACCESS read-write
     STATUS current
     DESCRIPTION
         "Disable/Enable access to allow that the WLAN PCs in the different SSIDs
          can communicate with each other."
     ::= { arrisWrmDevWlan 8 }

arrisWrmDevWlanQoSConfig OBJECT IDENTIFIER
     ::= { arrisWrmDevWlan 9 }

arrisWrmDevWlanQoSConfigEnable OBJECT-TYPE
     SYNTAX INTEGER {
         enable(1),
         disable(2)
     }
     MAX-ACCESS read-write
     STATUS current
     DESCRIPTION " enable/disable QoS bandwidth control. "
     ::= { arrisWrmDevWlanQoSConfig 1 }

arrisWrmDevWlanQoSConfigTotalBandwidth OBJECT-TYPE
     SYNTAX Integer32
     MAX-ACCESS read-write
     STATUS current
     DESCRIPTION " Router Wan Iface total bandwith: 100 - 100000 kbps"
     ::= { arrisWrmDevWlanQoSConfig 2 }

arrisWrmDevWlanQoSConfigHomeBandwidth OBJECT-TYPE
     SYNTAX Integer32
     MAX-ACCESS read-write
     STATUS current
     DESCRIPTION " Router Lan/WLAN1 bandwith: 100 - 100000 kbps"
     ::= { arrisWrmDevWlanQoSConfig 3 }

arrisWrmDevWlanQoSConfigPrivateBandwidth OBJECT-TYPE
     SYNTAX Integer32
     MAX-ACCESS read-write
     STATUS current
