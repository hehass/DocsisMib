     ::= { arrisWrmDevWrmp 11 }

--
-- The following group describes the MIB objects which show the System Information
--
arrisWrmDevFW OBJECT-TYPE
     SYNTAX  OCTET STRING
     MAX-ACCESS read-only
     STATUS current
     DESCRIPTION "Shows WRM Firmware version."
     ::= { arrisWrmDevSystem 1 }

arrisWrmDevBootVer OBJECT-TYPE
     SYNTAX  OCTET STRING
     MAX-ACCESS read-only
     STATUS current
     DESCRIPTION "Shows WRM BOOT version."
     ::= { arrisWrmDevSystem 2 }

arrisWrmDevHWName OBJECT-TYPE
     SYNTAX  OCTET STRING
     MAX-ACCESS read-only
     STATUS current
     DESCRIPTION "Shows WRM Hardware Name."
     ::= { arrisWrmDevSystem 3 }

arrisWrmDevTime OBJECT-TYPE
     SYNTAX  TimeTicks
     MAX-ACCESS read-only
     STATUS current
     DESCRIPTION "Shows the uptime of the WRM since the last reboot."
     ::= { arrisWrmDevSystem 4 }

arrisWrmDevLanMac OBJECT-TYPE
     SYNTAX  OCTET STRING
     MAX-ACCESS read-only
     STATUS current
     DESCRIPTION "Shows WRM Hardware Name."
     ::= { arrisWrmDevSystem 5 }

arrisWrmDevWanMac OBJECT-TYPE
     SYNTAX  OCTET STRING
     MAX-ACCESS read-only
     STATUS current
     DESCRIPTION "Shows WRM Hardware Name."
     ::= { arrisWrmDevSystem 6 }

arrisWrmDevFactoryReset OBJECT-TYPE
     SYNTAX INTEGER {
         true(1),
         false(2)
     }
     MAX-ACCESS read-write
     STATUS current
     DESCRIPTION
         "If set to True(1), the WRM will revert setttings to factory default and then
          auto-reboot. When read, False(2) will always be returned."
     ::= { arrisWrmDevSystem 7 }

arrisWrmDevRefresh OBJECT-TYPE
     SYNTAX INTEGER {
         true(1),
         false(2)
     }
     MAX-ACCESS read-write
     STATUS current
     DESCRIPTION
         "If set to True(1), the WRM will copy GUI RAM value to CLI RAM value.
          When read, False(2) will always be returned.(similar to sdc refresh).
          And it will be in effect right now."
     ::= { arrisWrmDevSystem 8 }

END
